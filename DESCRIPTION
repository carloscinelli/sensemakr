Package: sensemakr
Type: Package
Title: Sensitivity Analysis Tools for OLS
Date: 2019-07-26
Version: 0.1.1
Authors@R: c(
  person("Carlos", "Cinelli", role = c("aut", "cre"), email = "carloscinelli@hotmail.com"),
